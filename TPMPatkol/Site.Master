        <script type="text/javascript">
            $(document).ready(function () {
                var site_ref = getUrlVars()["site_ref"];
                var username = sessionStorage.getItem("username");
                if (username == null) {
                    window.location.href = "LoginTPMPortal.aspx";
                }


            });

            function CallAjaxReturnData(method, Setdata) {
                var data = "";
                $.ajax({
                    type: "POST",
                    async: false,
                    global: false,
                    url: method,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    data: JSON.stringify(Setdata),
                    success: function (response) {
                        if (response.d.length > 0) { data = response.d; }
                        else { data = "0"; }
                    }
                });
                return data;
            }


            function inputsearchText(url, data, text) {
                $.ajax({
                    type: "POST",
                    async: true,
                    url: url,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    data: JSON.stringify(data),
                    success: text
                });
            }

            function openNav() {
                document.getElementById("mySidenav").style.width = "300px";
            }

            function closeNav() {
                document.getElementById("mySidenav").style.width = "0";
            }

            function getUrlVars() {
                var vars = [], hash;
                var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                for (var i = 0; i < hashes.length; i++) {
                    hash = hashes[i].split('=');
                    vars.push(hash[0]);
                    vars[hash[0]] = hash[1];
                }
                return vars;
            }

            function getAllUrlParams(sParam) {

                var sPageURL = decodeURIComponent(window.location.search.substring(1)),
                    sURLVariables = sPageURL.split('&'),
                    sParameterName,
                    i;

                for (i = 0; i < sURLVariables.length; i++) {
                    sParameterName = sURLVariables[i].split('=');

                    if (sParameterName[0] === sParam) {
                        return sParameterName[1] === undefined ? true : sParameterName[1];
                    }
                }
            }

            function numberWithCommas(number) {
                var parts = number.toString().split(".");
                parts[0] = parts[0].replace(/\B(?=(\d{3})+(?!\d))/g, ",");
                return parts.join(".");
            }

            function detect_enter_keyboard(event) {
                var key_board_keycode = event.which || event.keyCode;
                if (key_board_keycode == 13) {
                    event.preventDefault();
                }
            }

            function CallAjaxReturnData(method, Setdata) {
                var data = "";
                $.ajax({
                    type: "POST",
                    async: false,
                    global: false,
                    url: method,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    data: JSON.stringify(Setdata),

            }


            function insertFile(data, name, Type) {
                var formData = new FormData();
                formData.append('file', data);
                formData.append('name', name);
                formData.append('Type', Type);

                $.ajax({
                    type: 'post',
                    async: false,
                    url: 'UploadFile.ashx',
                    data: formData,
                    success: function (status) {
                        //alert("Upload Complete");
                        //if (check == "True") { 
